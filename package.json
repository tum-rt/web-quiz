{
  "name": "web-quiz",
  "version": "0.1.0",
  "description": "Mobile-first web quiz",
  "author": "Hans Kirchner <h.kirchner@tum.de>",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "webpack",
    "buildp": "webpack -p",
    "watch": "webpack --watch",
    "dev": "NODE_ENV=development node server example-quiz",
    "doc": "apidoc -i lib/ -o apidoc/",
    "test": "mocha lib/test",
    "karma": "karma start karma.conf.js --single-run",
    "karma-watch": "karma start karma.conf.js --no-single-run",
    "lint": "eslint ./ && tslint -c tslint.json 'app/**/*.ts'",
    "saucelabs": "karma start karma-saucelabs.conf.js",
    "coverage": "istanbul cover _mocha --report lcovonly -- lib/test",
    "publish_to_coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "compression": "^1.6.2",
    "css-loader": "^0.28.1",
    "express": "^4.15.3",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "helmet": "^3.6.0",
    "json-loader": "^0.5.4",
    "mathjax-node": "^1.1.0",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-rc.1",
    "spectre.css": "^0.2.12",
    "style-loader": "^0.17.0",
    "svgo": "^0.7.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.3.2",
    "vue": "^2.3.3",
    "vue-i18n": "^6.1.3",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.3.3",
    "vue-ts-loader": "0.0.3",
