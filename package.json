    "@types/core-js": "^0.9.41",
    "@types/mocha": "^2.2.41",
    "apidoc": "^0.17.6",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.5",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.1",
    "tslint": "^5.2.0",
